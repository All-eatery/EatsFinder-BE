{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src/main/nest",
  "compilerOptions": {
    "assets": [
      {
        "include": "**/*.hbs",
        "outDir": "dist",
        "watchAssets": true
      }
    ],
    "deleteOutDir": true
  }
}
